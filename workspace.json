{
  "version": 2,
  "projects": {
    "admin-pannel": {
      "root": "apps/admin-pannel",
      "sourceRoot": "apps/admin-pannel",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/admin-pannel",
            "outputPath": "dist/apps/admin-pannel"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "admin-pannel:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "admin-pannel:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "admin-pannel:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin-pannel/**/*.{ts,tsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/admin-pannel"
          ],
          "options": {
            "jestConfig": "apps/admin-pannel/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin-pannel-e2e": {
      "root": "apps/admin-pannel-e2e",
      "sourceRoot": "apps/admin-pannel-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-pannel-e2e/cypress.json",
            "tsConfig": "apps/admin-pannel-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin-pannel:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin-pannel:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin-pannel-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "prefix": "backend",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/backend",
            "main": "apps/backend/src/main.ts",
            "tsConfig": "apps/backend/tsconfig.app.json",
            "assets": [
              "apps/backend/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend/src/environments/environment.ts",
                  "with": "apps/backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backend:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/backend/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/backend"
          ],
          "options": {
            "jestConfig": "apps/backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "mobile": {
      "root": "apps/mobile",
      "sourceRoot": "apps/mobile/src",
      "projectType": "application",
      "schematics": {},
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start mobile"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": [
            "apps/mobile/build"
          ],
          "options": {
            "entryFile": "apps/mobile/index.js",
            "platform": "ios",
            "bundleOutput": "dist/apps/mobile/ios/index.bundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/mobile/android/app/build/outputs/bundle",
            "apps/mobile/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/mobile/index.js",
            "platform": "android",
            "bundleOutput": "dist/apps/mobile/android/index.bundle"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/mobile/**/*.{js,ts,tsx}"
            ]
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/mobile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "main-app": {
      "root": "apps/main-app",
      "sourceRoot": "apps/main-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/main-app",
            "index": "apps/main-app/src/index.html",
            "main": "apps/main-app/src/main.tsx",
            "polyfills": "apps/main-app/src/polyfills.ts",
            "tsConfig": "apps/main-app/tsconfig.app.json",
            "assets": [
              "apps/main-app/src/favicon.ico",
              "apps/main-app/src/assets"
            ],
            "styles": [
              "apps/main-app/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/main-app/src/environments/environment.ts",
                  "with": "apps/main-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "main-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "main-app:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/main-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/main-app"
          ],
          "options": {
            "jestConfig": "apps/main-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "main-app-e2e": {
      "root": "apps/main-app-e2e",
      "sourceRoot": "apps/main-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/main-app-e2e/cypress.json",
            "tsConfig": "apps/main-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "main-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "main-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/main-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "admin-pannel"
}